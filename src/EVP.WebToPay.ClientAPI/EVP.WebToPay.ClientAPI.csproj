<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>paysera-bank-client</PackageId>
    <Authors>dima-horror</Authors>
    <Description>WebToPay Lib DotNet is a library that will allow you to make payment requests to the Paysera website.</Description>
    <PackageProjectUrl>https://github.com/dima-horror/webtopay-lib-dotnet</PackageProjectUrl>
    <PackageIcon>paysera.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\paysera.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="EVP.WebToPay.ClientAPI.snk" />
  </ItemGroup>
 
</Project>